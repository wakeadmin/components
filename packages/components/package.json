{
  "name": "@wakeadmin/components",
  "version": "0.4.1",
  "description": "wakeadmin 组件库",
  "module": "./dist/index.js",
  "main": "./dist/common/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "style",
    "dist"
  ],
  "sideEffects": [
    "**/*.scss",
    "**/*.css"
  ],
  "scripts": {
    "start": "tsc -b tsconfig.build.json --watch",
    "prebuild": "rimraf dist .tsBuildInfo",
    "test": "vitest",
    "build": "npm run build:es && npm run build:common",
    "build:es": "tsc -b tsconfig.build.json",
    "build:common": "tsc -b tsconfig.build.common.json",
    "type-check": "tsc"
  },
  "keywords": [
    "wakeadmin",
    "components"
  ],
  "author": "ivan-lee",
  "license": "MIT",
  "peerDependencies": {
    "vue": ">=2.7.0",
    "element-plus": "^2.2.12",
    "element-ui": "^2.15.9"
  },
  "peerDependenciesMeta": {
    "element-ui": {
      "optional": true
    },
    "element-plus": {
      "optional": true
    }
  },
  "dependencies": {
    "@wakeadmin/element-adapter": "workspace:^0.2.0",
    "@wakeadmin/demi": "latest",
    "@wakeadmin/h": "latest",
    "@wakeadmin/icons": "latest",
    "@wakeadmin/utils": "latest"
  },
  "devDependencies": {
    "vitest": "^0.23.1"
  }
}
